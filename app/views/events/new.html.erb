<br><br>
<%= form_with model: @event do |form| %>
  <div>
    <%= form.label :name %><br>
    <%= form.text_field :name%>
  </div>
  <div>

  <div>
    <%= form.label :description %><br>
    <%= form.text_field :description%>
  </div>
  <div>
    <%= form.label :date%><br>
     <%= form.date_field  :date%><br>
    <% @event.errors.full_messages_for(:date).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>
  <div>
  <div><br>
    <%= form.submit %>
  </div>
<% end %>